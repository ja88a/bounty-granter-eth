{
  "name": "@bounty-granter-eth/dataset",
  "version": "0.1.0",
  "description": "Data management library for Bounty Granter",
  "main": "index.js",
  "author": "@ja88a Jabba01",
  "license": "AGPL-3.0-or-later",
  "private": true,
  "scripts": {
    "prepublishOnly": "yarn build && yarn test",
    "setup": "yarn build",
    "build": "rimraf ./build && tsc -b",
    "build:json-schema": "",
    "lint": "eslint --ignore-path ./.eslintignore . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint --ignore-path ./.eslintignore . --ext .js,.jsx,.ts,.tsx --fix",
    "prettier-format": "run-script-os",
    "prettier-format:win32": "prettier --config .prettierrc \"./src/**/*.ts\" --write",
    "prettier-format:darwin:linux": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "prettier-format:default": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "prettier-watch": "run-script-os",
    "prettier-watch:win32": "onchange \"src/**/*.ts\" -- prettier --write {{changed}}",
    "prettier-watch:darwin:linux": "onchange 'src/**/*.ts' -- prettier --write {{changed}}",
    "prettier-watch:default": "onchange 'src/**/*.ts' -- prettier --write {{changed}}",
    "server": "node -e 'require(\"./index.js\").server()'",
    "server:dist": "node -e 'require(\"./dist/index.js\").server()'",
    "start:dev": "nodemon",
    "start": "npm run build && node build/index.js",
    "test": "jest",
    "test:dev": "jest --watchAll"
  },
  "dependencies": {
    "@bounty-granter-eth/dataset": "link:",
    "cbor-x": "^1.5.2",
    "class-transformer": "^0.5.1",
    "class-validator": "0.14.0",
    "js-yaml": "^4.1.0",
    "keccak256": "^1.0.6",
    "merkletreejs": "^0.3.9",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@types/chai": "^4.3.4",
    "@types/jest": "^29.5.0",
    "@types/js-yaml": "^4.0.5",
    "@types/mocha": "^9.1.1",
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "cbor": "^8.1.0",
    "chai": "^4.3.7",
    "class-validator-jsonschema": "^5.0.0",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jest": "^29.3.1",
    "mocha": "^9.2.2",
    "nodemon": "^2.0.22",
    "onchange": "^7.1.0",
    "prettier": "^2.8.7",
    "rimraf": "^4.4.1",
    "rlp": "^4.0.0-beta.2",
    "run-script-os": "^1.1.6",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.3"
  }
}
