{
  "name": "@bounty-granter-eth/webui",
  "version": "0.1.0",
  "private": true,
  "homepage": ".",
  "keywords": [
    "ethers",
    "mui",
    "rainbowkit",
    "react",
    "tailwindcss",
    "wagmi"
  ],
  "scripts": {
    "build": "react-scripts build",
    "clean": "npx shx rm -rf ./dist ./build ./*.tsbuildinfo ./yarn-error.log*",
    "eject": "react-scripts eject",
    "ipfs": "node ./scripts/ipfs.js",
    "lint": "eslint --config ./.eslintrc.js --ignore-path ./.eslintignore ./src/**/*",
    "prestart": "node ./scripts/create_contracts.js",
    "prettier": "npx prettier --write . '!(node_module|build)/**/*'",
    "reset": "yarn clean && npx shx rm -rf ./node_modules ./yarn.lock ./package-lock.json ./pnpm-lock.yaml",
    "serve": "npx serve -s build",
    "ship": "yarn surge",
    "surge": "npx shx cp build/index.html build/200.html && surge ./build",
    "s3": "node ./scripts/s3.js",
    "start": "react-scripts start",
    "test": "react-scripts test",
    "watch": "node ./scripts/watch.js"
  },
  "dependencies": {
    "@apollo/react-hooks": "^4.0.0",
    "@aut-protocol/d-aut": "^1.3.2",
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@mui/icons-material": "^5.10.3",
    "@mui/material": "^5.10.5",
    "@mui/system": "5.4.1",
    "@mui/x-data-grid": "5.17.4",
    "@rainbow-me/rainbowkit": "^0.5.2",
    "apollo-boost": "^0.4.9",
    "apollo-client": "^2.6.10",
    "apollo-utilities": "^1.3.4",
    "dotenv": "^16.0.2",
    "ethers": "^5.7.0",
    "graphql": "^15.0.0",
    "ipfs-http-client": "^58.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.1.8",
    "wagmi": "^0.6.6",
    "undici": "^5.10.0",
    "web-vitals": "^3.0.1"
  },
  "devDependencies": {
    "@apollo/client": "^3.6.9",
    "@babel/core": "^7.19.0",
    "@babel/plugin-syntax-flow": "^7.18.6",
    "@babel/plugin-transform-react-jsx": "^7.19.0",
    "@babel/preset-env": "^7.19.0",
    "@testing-library/dom": "^8.17.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.0.1",
    "@types/node": "^17.0.35",
    "@types/react": "^18.0.20",
    "@types/react-dom": "^18.0.6",
    "autoprefixer": "^10.4.9",
    "chalk": "^5.0.1",
    "eslint": "^8.15.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.8",
    "eslint-plugin-react-hooks": "^4.6.0",
    "postcss": "^8.4.16",
    "prettier": "^2.7.1",
    "react-native": "^0.70.0",
    "react-scripts": "^5.0.1",
    "s3-folder-upload": "^2.3.5",
    "shx": "^0.3.4",
    "serve": "^14.0.1",
    "surge": "^0.23.1",
    "typescript": "^4.8.3"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
