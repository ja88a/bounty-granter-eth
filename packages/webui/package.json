{
  "name": "@bounty-granter-eth/webui",
  "version": "0.1.0",
  "private": true,
  "homepage": ".",
  "keywords": [
    "ethers",
    "mui",
    "rainbowkit",
    "react",
    "tailwindcss",
    "wagmi"
  ],
  "scripts": {
    "build": "react-scripts build",
    "clean": "npx shx rm -rf ./dist ./build ./*.tsbuildinfo ./yarn-error.log*",
    "eject": "react-scripts eject",
    "ipfs": "node ./scripts/ipfs.js",
    "lint": "eslint --config ./.eslintrc.js --ignore-path ./.eslintignore ./src/**/*",
    "prestart": "node ./scripts/create_contracts.js",
    "prettier": "npx prettier --write . '!(node_module|build)/**/*'",
    "reset": "yarn clean && npx shx rm -rf ./node_modules ./yarn.lock ./package-lock.json ./pnpm-lock.yaml",
    "serve": "npx serve -s build",
    "ship": "yarn surge",
    "surge": "npx shx cp build/index.html build/200.html && surge ./build",
    "s3": "node ./scripts/s3.js",
    "start": "react-scripts start",
    "test": "react-scripts test",
    "watch": "node ./scripts/watch.js"
  },
  "dependencies": {
    "@apollo/react-hooks": "^4.0.0",
    "@aut-protocol/d-aut": "^1.6.7",
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.11.16",
    "@mui/system": "^5.11.16",
    "@mui/x-data-grid": "5.17.4",
    "@rainbow-me/rainbowkit": "^0.12.7",
    "apollo-boost": "^0.4.9",
    "apollo-client": "^2.6.10",
    "apollo-utilities": "^1.3.4",
    "dotenv": "^16.0.3",
    "ethers": "^5.7.2",
    "graphql": "^15.8.0",
    "ipfs-http-client": "^56.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.10.0",
    "tailwindcss": "^3.3.1",
    "undici": "^5.21.0",
    "wagmi": "^0.12.10",
    "web-vitals": "^3.3.1"
  },
  "devDependencies": {
    "@apollo/client": "^3.7.11",
    "@babel/core": "^7.21.4",
    "@babel/plugin-syntax-flow": "^7.21.4",
    "@babel/plugin-transform-react-jsx": "^7.21.0",
    "@babel/preset-env": "^7.21.4",
    "@testing-library/dom": "^9.2.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.0",
    "@types/node": "^17.0.45",
    "@types/react": "^18.0.33",
    "@types/react-dom": "^18.0.11",
    "autoprefixer": "^10.4.14",
    "chalk": "^5.1.2",
    "eslint": "^8.37.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "postcss": "^8.4.21",
    "prettier": "^2.8.7",
    "react-native": "^0.71.6",
    "react-scripts": "^5.0.1",
    "s3-folder-upload": "^2.3.5",
    "serve": "^14.2.0",
    "shx": "^0.3.4",
    "surge": "^0.23.1",
    "typescript": "^4.9.4"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 2 chrome version",
      "last 2 firefox version",
      "last 2 safari version"
    ]
  }
}
